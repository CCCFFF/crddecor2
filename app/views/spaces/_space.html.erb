

  <li class="media span4">
      <%= space.category %>  <%= link_to "Edit Room", edit_space_url(space.id), :class => "btn btn-info btn-xs" %>
      <ul class="media-list">
      <% space.space_pictures.each do |the_space_pic| %>
            <h3>Pictures to <%= "#{space.name}" %></h3> <%= link_to "Add Picture", new_space_picture_url, :class => "btn btn-info btn-xs" %>
        <li>
          <%= render '/space_pictures/space_picture', :space_pic => the_space_pic %>
        </li>
        <% end %>
    </ul>
    </li>

